#include "imx7d-19x19-ddr3-arm2.dts"

/ {
	sound {
		compatible = "fsl,imx7d-19x19-ddr3-arm2-wm8958",
			   "fsl,imx-audio-wm8958";
		model = "wm8958-audio";
		cpu-dai = <&sai2>;
		audio-codec = <&codec>;
		codec-master;
		hp-det-gpios = <&gpio1 13 1>;
	};
};

&iomuxc {
	pinctrl-0 = <&pinctrl_hog_1>;
};

&usdhc3 {
	no-1-8-v;
};

&sai2 {
	status = "okay";
};
