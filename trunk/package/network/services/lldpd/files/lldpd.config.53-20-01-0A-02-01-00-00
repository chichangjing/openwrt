config lldpd config
	option enable_cdp 1
	option enable_fdp 1
	option enable_sonmp 1
	option enable_edp 1
	
	option lldp_class 4
	option lldp_location "2:FR:6:Commercial Rd:3:Roseville:19:4"

	# if empty, the distribution description is sent
	# option lldp_description "OpenWrt System"

	# interfaces to listen on
	list interface "loopback"
	list interface "br-lan"
	list interface "eth0"
	list interface "Gex-1"
	list interface "Gex-2"
	list interface "Fex-3"
	list interface "Fex-4"
	list interface "Fex-5"
	list interface "Fex-6"
	list interface "Fex-7"
	list interface "Fex-8"
	list interface "Fex-9"
	list interface "Fex-10"
